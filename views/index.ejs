        .cloud {
            margin-top: 25px;
            width: 90px;
            height: 60px;
        }

        .cloud::before {
            width: 35px;
            height: 35px;
            top: -15px;
            left: 3px;
        }

        .cloud::after {
            width: 45px;
            height: 45px;
            top: -20px;
            right: 3px;
        }

        img {
            width: 30px;
        }

        h1 {
            font-size: 18px;
        }

        p {
            font-size: 10px;
        }
    }

