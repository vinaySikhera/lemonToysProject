    .container {
        width: 100%;
        margin: auto;
        background: white;
        padding: 20px;
        border-radius: 10px;
        box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);
    }

    h1 {
        text-align: center;
        font-size: 24px;
        font-weight: bold;
    }

    .container p {
        text-align: center;
    }

    .collection {

        width: 80%;
        padding-top: 50px;
        padding-bottom: 30px;
        display: flex;
        justify-content: space-around;
        margin: auto;
        margin-top: 20px;
    }

    .category {
        text-align: center;
    }

    .cloud {
        width: 120px;
        height: 80px;
        background: #b3e5fc;
        position: relative;
        display: flex;
        align-items: center;
        justify-content: center;
        border-radius: 30px;
        box-shadow: inset 0 -10px 10px rgba(0, 0, 0, 0.1);
    }

    .cloud::before,
    .cloud::after {
        content: '';
        position: absolute;
        background: #b3e5fc;
        border-radius: 50%;
    }

    .cloud::before {
        width: 50px;
        height: 50px;
        top: -25px;
        left: 10px;
        bottom: -25px;
    }

    .cloud::after {
        width: 60px;
        height: 60px;
        top: -30px;
        right: 10px;
    }

    img {
        width: 40px;
        height: auto;
        position: relative;
        z-index: 2;
    }

    p {
        margin-top: 20px;
        font-size: 14px;
    }


    @media screen and (max-width: 768px) {
        .collection {
            flex-direction: column;
            align-items: center;
            width: 100%;
            padding-top: 30px;
            padding-bottom: 20px;
        }

        .cloud {
            width: 100px;
            height: 70px;
        }

        .cloud::before {
            width: 40px;
            height: 40px;
            top: -20px;
            left: 5px;
        }

        .cloud::after {
            width: 50px;
            height: 50px;
            top: -25px;
            right: 5px;
        }

        img {
            width: 35px;
        }

        h1 {
            font-size: 20px;
        }

        p {
            font-size: 12px;
        }
    }


    @media screen and (max-width: 480px) {
        .container {
            padding: 15px;
        }

        .collection {

            padding-top: 20px;
            padding-bottom: 15px;
